
struct Bounds {
	min: Int
	max: Int
}

struct Result { bounds: Bounds }

def getBounds(res: Result*, data: Int*, size: Int): Void {
	var index = 0
	while (index < size) {
		var value = (data + index).*
		if (value < res.bounds.min) res.bounds.min = value
		if (value > res.bounds.max) res.bounds.max = value
		index = index + 1
	}
}
